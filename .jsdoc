{
  "source": {
    "include": [
      "src/core/"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "projectTitle": "como",
    "destination": "./docs/public/api",
    "encoding": "utf8",
    "template": "./node_modules/docdash"
  },
  "templates": {
    "default": {
      "useLongnameInNav": true
    },
    "footer": ""
  },
  "docdash": {
    "search": true,
    "typedefs": true,
    "sectionOrder": [
      "Namespaces",
      "Classes",
      "Global",
      "Modules",
      "Externals",
      "Events",
      "Mixins",
      "Tutorials",
      "Interfaces"
    ],
    "scripts": [
      "../api-styles.css"
    ],
    "meta": {
        "title": "como",
        "description": "Documentation of the como framework"
    },
    "menu": {
      "Website": {
        "href":"../",
        "class":"menu-item"
      },
      "Github": {
        "href": "https://github.com/ircam-ismm/como",
        "target":"_blank",
        "class":"menu-item"
      }
    }
  }
}
