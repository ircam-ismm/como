{
  "source": {
    "include": [
      "src/core/",
      "src/components/source-manager",
      "src/components/session-manager",
      "src/components/soundbank-manager"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "projectTitle": "como",
    "destination": "./docs/public/api",
    "encoding": "utf8",
    "template": "./node_modules/@ircam/jsdoc-template"
  },
  "templates": {
    "default": {
      "useLongnameInNav": true
    },
    "footer": ""
  },
  "docdash": {
    "logo": "/como/logo-200x200.png",
    "favicon": "/como/favicon.ico",
    "collapse": true,
    "search": true,
    "typedefs": true,
    "sectionOrder": [
      "Namespaces",
      "Classes",
      "Global",
      "Modules",
      "Externals",
      "Events",
      "Mixins",
      "Tutorials",
      "Interfaces"
    ],
    "meta": {
      "title": "como",
      "description": "Documentation of the como framework"
    },
    "menu": {
      "Website": {
        "href":"../",
        "class":"menu-item"
      },
      "Github": {
        "href": "https://github.com/ircam-ismm/como",
        "target":"_blank",
        "class":"menu-item"
      }
    }
  }
}
